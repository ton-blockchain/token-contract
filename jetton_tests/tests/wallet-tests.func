	int transfer_fee   = jetton_gas_fee * 2;
	int forward_fee    = one_unit / 10;
	int forward_amount = one_unit / 4;
	int excesses	   = 1;
	int msg_expected   = forward_amount > 0 ? 2 : 1;
	int msg_value      = transfer_fee + muldiv(forward_fee, 3, 2) * msg_expected + forward_amount + excesses; ;;Account for everythin except storage fee
