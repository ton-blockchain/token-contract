	;;Expect fail
	gas_used 	  += transfer_test_msg_value( msg_value, forward_fee, forward_amount, true );

	;; Verifying error trigger
	gas_used	  += transfer_test_msg_value( msg_value + jetton_min_storage, forward_fee, forward_amount, false ) ;

	return gas_used;
}


